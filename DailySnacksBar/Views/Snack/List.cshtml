@model IEnumerable<DailySnacksBar.Models.Snack>

@{
    ViewData["Title"] = "All Snacks";
}

<h1>@ViewData["Title"]</h1>


@foreach (var item in Model) {
       
    <div>
        <h4>@item.Name</h4>
        <p><img src="@item.ImgUrl"/></p>
        <h3>Price: @item.Price.ToString("c")</h3>
    </div>
}